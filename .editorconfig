# http://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8
end_of_line = lf

[*.bat]
end_of_line = crlf
indent_style = tab
indent_size = tab

[*.py]
charset = latin1

[LICENSE]
insert_final_newline = false

[Makefile]
indent_style = tab
indent_size = tab
